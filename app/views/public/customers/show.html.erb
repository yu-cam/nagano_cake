<div class="container">
  <div class="row">
<h1>マイページ</h1>
<h4>>登録情報</h4>

<%= link_to "編集する", edit_public_customer_path, class: "btn-sm btn-success" %>
<table class="table table-bordered">
		<tr>
			<th>氏名</th>
			<td><%= @customer.last_name %></td>
		</tr>
		<tr>
			<th>カナ</th>
			<td><%= @customer.last_name_kana %></td>
		</tr>
		<tr>
			<th>郵便番号</th>
			<td><%= @customer.postal_code %></td>
		</tr>
		<tr>
		    <th>住所</th>
		    <td><%= @customer.address %></td>
		</tr>
		<tr>
            <th>電話番号</th>
			<td><%= @customer.telephone_number %></td>
		</tr>
		<tr>
		    <th>メールアドレス</th>
		    <td><%= @customer.email %></td>
		</tr>
 </table>
<h4>配送先</h4>
<h4>注文履歴</h4>
</div>
</div>