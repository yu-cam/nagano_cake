<div class="container">
  <div class="row">
  	<h2>商品一覧</h2>
	<% @item.each do |item| %>
	<tr>
	<td><%= attachment_image_tag item, :image, :fill, 150, 150, format: 'jpeg' %></td>
    <td><%= link_to item.name, public_item_path(item.id) %></td>
    <td><%= item.price %></td>
    </tr>
	<% end %>
  </div>
</div>